# LaunchLine Starter - Main Environment Configuration
# Copy this file to .env and customize as needed

# Frontend API URL (for development)
VITE_API_URL=http://localhost:8000

# Backend settings
DEBUG=True
SECRET_KEY=dev-secret-key-change-in-production
DATABASE_URL=postgres://postgres:postgres@localhost:5432/launchline_starter_dev
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173,http://localhost:3000,http://127.0.0.1:3000,http://localhost,http://127.0.0.1

# Django settings module
DJANGO_SETTINGS_MODULE=config.settings.dev

# Development helpers
COMPOSE_FILE=ops/docker-compose.dev.yml
COMPOSE_PROJECT_NAME=launchline_starter
