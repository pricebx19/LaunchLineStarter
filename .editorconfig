# EditorConfig for LaunchLine Starter
# https://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.{yml,yaml}]
indent_size = 2

[*.{json,js,ts,vue}]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[Dockerfile]
indent_size = 2

[Makefile]
indent_style = tab

[*.{html,css,scss}]
indent_size = 2